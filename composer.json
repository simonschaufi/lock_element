	"type": "typo3-cms-extension",
	"description": "Lock Element",
	"license": ["GPL-2.0+"],
	"keywords": ["TYPO3 CMS"],
	"require": {
		"typo3/cms-core": "^8.7 || ^9.5"
	},
	"autoload": {
		"psr-4": {
			"SimonSchaufi\\LockElement\\": "Classes"
		}
